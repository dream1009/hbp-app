# 所有环境默认

# 后台配置的Token key
VUE_APP_TOKEN_KEY = 'X-Authorization-access_token'

# 申请应用时分配的AppKey
VUE_APP_CLIENT_ID = 'ibps'
# 申请应用时分配的AppSecret
VUE_APP_CLIENT_SECRET = '58b65297-3467-0859-8337-8cbaf81ef68a'
# 授权方式:authorization_code ：授权码方式，  password_credentials：密码模式
VUE_APP_GRANT_TYPE = 'authorization_code'

# 公众号模式，仅用作公众号的时候启用
VUE_APP_IS_OFFICIAL_ACCOUNT = false
# 开启微信登录,免登录
VUE_APP_IS_WECHAT_LOGIN = true

# 如果打包移动应用请设置为true，cookie丢失问题
VUE_APP_IS_APP = true


# 需要登录，获取数据的前缀.
VUE_APP_API_KEY = '/webapi'
# 匿名登录，获取数据的前缀.如果webpack或者nginx 要加上/api
VUE_APP_ANONYMOUS_API_KEY = ''

#是否app端免登录
VUE_APP_LOGIN_FREE = 'login_free_flag'
